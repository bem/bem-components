block attach, elem control {

    mix: [{
        block: 'button',
        elem: 'control'    
    }]

    tag: 'input'

    attrs: {
        var a = {
            type: 'file',
            name: this._name
        };

        !!this._disabled && (a.disabled = 'disabled');
        this._tabindex && (a.tabindex = this._tabindex);

        a['aria-labelledby'] = this._textId;

        return a;
    }

}
