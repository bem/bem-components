block('menu')(
    def()(function() {
        applyNext({
            _menuMods : {
                theme : this.mods.theme,
                disabled : this.mods.disabled
            }
        });
    }),
    attrs()(function() {
        var attrs = { role : 'menu' };
        this.mods.disabled || (attrs.tabindex = 0);
        return attrs;
    }),
    js()(true),
    mix()([{ elem : 'control' }])
);
